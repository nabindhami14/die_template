  "main": "index.js",
  "scripts": {
    "compile": "cd protos && protoc --js_out=import_style=commonjs,binary:../build  *.proto",
    "bundle": "cd protos && ../node_modules/.bin/pbjs -t static-module -w commonjs -o ../bundle.js *.proto",
